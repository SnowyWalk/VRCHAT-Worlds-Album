{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  
  "AppPaths": {
    "ThumbImageDir": "static/Thumb",
    "ViewImageDir": "static/View",
