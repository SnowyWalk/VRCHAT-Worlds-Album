    "ScanFolderPath": "static/Worlds"
  },
  "Image": {
    "ThumbQuality": 15,
    "ViewQuality": 95
  },
  "Cache": {
    "WorldMetadataTTL": "24:00:00",
    "ScanInterval": "00:01:00"
  }
}
