{
  "Logging": {
    "LogLevel": {
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "Microsoft.EntityFrameworkCore.Query": "Warning",
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "AppPaths": {
    "OriginImageDir": "static/Worlds",
    "ThumbImageDir": "static/Thumb",
    "ViewImageDir": "static/View",
    "DatabaseJsonPath": "Database/Data.json",
    "DatabaseJsonTempPath": "Database/Data.json.tmp",
    "ScanFolderPath": "static/Worlds"
  },
  "Image": {
    "ThumbQuality": 15,
    "ViewQuality": 95
  },
  "Cache": {
    "WorldMetadataTTL": "24:00:00",
    "ScanInterval": "00:00:05"
  }
}
